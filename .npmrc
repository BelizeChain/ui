# npm configuration for BelizeChain UI
# Registry configuration
registry=https://registry.npmjs.org/

# Workspace configuration
workspaces=true

# Save exact versions
save-exact=true

# Engine strict (enforce Node.js version)
engine-strict=true

# Progress indicator
progress=true

# Audit level (warn about vulnerabilities)
audit-level=moderate

# Fund display
fund=false
